{
  "name": "yondako",
  "private": true,
  "scripts": {
    "dev": "next dev --experimental-https -H local.yondako.com",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint --write",
    "fmt": "biome format",
    "pages:build": "bunx @cloudflare/next-on-pages",
    "preview": "bun pages:build && wrangler pages dev",
    "deploy": "bun pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts",
    "generate:schema": "drizzle-kit generate --dialect sqlite --out ./src/db/migrations --schema",
    "svgo": "bunx svgo ./src/assets/icons/",
    "drizzle:studio": "bun --env-file=.env.local  run drizzle-kit studio",
    "storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "dependencies": {
    "@auth/drizzle-adapter": "^1.4.2",
    "@lottiefiles/react-lottie-player": "^3.5.4",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@react-spring/web": "^9.7.4",
    "budoux": "^0.6.2",
    "cloudflare": "^3.5.0",
    "drizzle-orm": "^0.32.1",
    "fast-xml-parser": "^4.4.1",
    "next": "14.2.5",
    "next-auth": "^5.0.0-beta.20",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-use": "^17.5.1",
    "server-only": "^0.0.1",
    "sharp": "^0.33.4",
    "tailwind-merge": "^2.4.0",
    "valibot": "^0.37.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@cloudflare/next-on-pages": "^1.13.1",
    "@cloudflare/workers-types": "^4.20240725.0",
    "@happy-dom/global-registrator": "^14.12.3",
    "@libsql/client": "^0.7.0",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-interactions": "^8.2.8",
    "@storybook/addon-links": "^8.2.8",
    "@storybook/blocks": "^8.2.8",
    "@storybook/nextjs": "^8.2.8",
    "@storybook/react": "^8.2.8",
    "@storybook/test": "^8.2.8",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.0",
    "@types/bun": "^1.1.6",
    "@types/node": "^20.14.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.24.0",
    "lefthook": "^1.7.11",
    "postcss": "^8.4.40",
    "storybook": "^8.2.8",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4",
    "vercel": "^36.0.0",
    "wrangler": "^3.68.0"
  },
  "overrides": {
    "string-width": "4.2.3"
  }
